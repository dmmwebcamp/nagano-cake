<div class="container">
  <h3>ジャンル編集</h3>
  <div class="row">
    
      <%= form_with model: @genre, url: admin_genre_path, method: :put, local: true do |f| %>
      <table>
        <th>ジャンル名</th>
        <th><%= f.text_field :name %></th>
        <th><%= f.submit '変更を保存' , class: 'btn btn-success' %></th>
      </table>
      <% end %>
  </div>
</div